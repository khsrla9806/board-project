<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.module.admin.transfer.repository.EmailRepository">

    <insert id="save" parameterType="com.module.admin.transfer.dto.EmailTransfer">
        INSERT INTO email (sender_id, receiver_id, title, content)
        VALUES (#{sender.id}, #{receiver.id}, #{title}, #{content})
    </insert>
</mapper>